{
  "ServiceOptions": {
    // Type your service name, ensure it is unique name
    "Name": "WebsiteCMS",
    // Your version, should match with configuration version folder
    "Version": "v1.0",
    // Your Saturn Server Url
    "SaturnEndpoint": "https://lp.saturn",
    // Disable check SSL via HTTP2
    "BypassSSL": true,
    "RetryCount": 5,
    "DelayRetry": 1000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
